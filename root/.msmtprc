defaults
port            587
host            mail.example.org
auth            on
tls             on
tls_starttls    on
tls_trust_file  /etc/ssl/certs/ca-certificates.crt
logfile         /var/log/msmtp

account         alerts
from            alerts@example.org
user            alerts@example.org
password        full-text-password


account default : alerts

